{
  "name": "Baldr_Web",
  "version": "0.0.0",
  "scripts": {
    "build:dev": "npm run clean:app && npm run webpack -- --env development --config config/webpack.dev.js --progress --profile",
    "build:dev-dll": "npm run clean:dll && npm run webpack -- --env development --config config/webpack.vendor.js --progress --profile",
    "clean:app": "npm run rimraf -- wwwroot/dist/app/*",
    "clean:dll": "npm run rimraf -- wwwroot/dist/dll/*",
    "clean:all": "npm run rimraf -- wwwroot/dist/*",
    "rimraf": "rimraf",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "test": "karma start config/karma.conf.js"
  },
  "dependencies": {
    "aurelia-framework": "^1.1.4",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "css": "^2.2.1",
    "es6-shim": "^0.35.1",
    "event-source-polyfill": "^0.0.7",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.1",
    "preboot": "^4.5.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.0.1",
    "systemjs": "^0.20.12",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/node": "^6.0.42",
    "@types/webpack": "^2.2.15",
    "@types/webpack-env": "^1.13.0",
    "add-asset-html-webpack-plugin": "^2.0.1",
    "awesome-typescript-loader": "^3.0.0",
    "css-loader": "^0.25.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-rc",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^4.1.0"
  }
}
